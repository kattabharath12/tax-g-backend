{
  "name": "tax_filing_backend",
  "version": "1.0.0",
  "description": "Backend API for Tax Filing Application",
  "main": "dist/server.js",
  "scripts": {
    "build": "npx prisma generate && tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "db:migrate": "npx prisma migrate deploy",
    "db:generate": "npx prisma generate",
    "db:seed": "ts-node src/scripts/seed.ts",
    "db:reset": "npx prisma migrate reset --force",
    "db:studio": "npx prisma studio",
    "postinstall": "npx prisma generate"
  },
  "keywords": [
    "tax",
    "filing",
    "api",
    "backend",
    "typescript",
    "prisma",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@google-cloud/documentai": "^8.8.0",
    "@prisma/client": "^5.19.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.5.4",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "next-auth": "^4.24.7",
    "prisma": "^5.19.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  }
}
